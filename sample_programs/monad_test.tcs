number x = 0

number(number, number, number(number)) executeOperationOnXYtimes = number(number x, number y, number(number) operation) {
  number i = 0
  while i < y {
    x = operation(x)
    i = i + 1
  }
  return x
}

number(number) incrementAndLogNumber = number(number x) {
  number incremented = x + 1
  print(incremented)
